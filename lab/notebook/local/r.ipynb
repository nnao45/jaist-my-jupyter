   "source": [
    "import pyper\n",
    "import pandas as pd\n",
    "\n",
    "# Python で CSV のデータを読み出す\n",
    "wine = pd.read_csv(\"wine.csv\")\n",
    "\n",
    "# R のインスタンスを作る\n",
    "r = pyper.R(use_pandas='True')\n",
    "\n",
    "# Python のオブジェクトを R に渡す\n",
    "r.assign(\"data\", wine)\n",
    "\n",
    "# R のソースコードを実行する\n",
    "r(\"source(file='scatter.R')\")"
   ]
